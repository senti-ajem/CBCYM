<%- include('partials/header.ejs') %>

<style>
  body{
      background-color: #FF9843;
  }
  
  </style>

<body>
    <h1>Edit Feed</h1>
    <form action="/update-feeds/<%= feedsId %>" method="post">
        <label for="name">Name:</label>
        <input type="text" id="name" name="name" value="<%= existingName %>" required><br><br>
        <label for="content">Content:</label>
        <textarea id="content" name="content" required><%= existingContent %></textarea><br><br>
        <input type="submit" value="Update">
    </form>
</body>
</html>

